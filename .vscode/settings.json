{
  "cline.customInstructions": "${workspaceFolder}/.cline/custom-instructions.md",
  "cline.projectConfig": "${workspaceFolder}/.cline/config.json",
  "files.associations": {
    "*.md": "markdown"
  },
  "markdown.preview.breaks": true,
  "markdown.preview.linkify": true,
  "files.exclude": {
    "**/.git": true,
    "**/.obsidian/cache": true,
    "**/*.pickle": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.obsidian/cache": true,
    "**/*.pickle": true,
    "**/token.json": true
  },
  "python.defaultInterpreterPath": "python3",
  "python.analysis.extraPaths": [
    "${workspaceFolder}/scripts/google-api"
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.obsidian/cache/**": true,
    "**/*.pickle": true
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": false,
      "strings": false,
      "other": false
    }
  }
}
